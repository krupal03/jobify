@model IEnumerable<Jobify.Models.AspNetUserRole>

@{
    ViewBag.Title = "Index";
}

<div class="container" style="margin-top:7%;">
    <h2>All</h2>

    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AssignAt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AspNetRole.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AspNetUser.FullName)
            </th>
          
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.AssignAt)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.AspNetRole.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.AspNetUser.FullName)
                </td>
               
            </tr>
        }

    </table>
    </div>